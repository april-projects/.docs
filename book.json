{
  "title": "April",
  "description": "April 项目组文档",
  "author": "April",
  "output.name": "site",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": ".",
  "links": {
    "sidebar": {
      "框架师": "https://www.mobaijun.com",
      "GitHub": "https://github.com/mobaijun",
      "Gitee": "https://gitee.com/mobaijun",
      "寻·妙": "https://nav.mobaijun.com"
    }
  },
  "plugins": [
    "code",
    "popup",
    "prism",
    "-lunr",
    "gtalk",
    "anchors",
    "donate",
    "favicon",
    "-search",
    "github",
    "sharing",
    "minifier",
    "splitter",
    "search-pro",
    "-highlight",
    "livereload",
    "insert-logo",
    "hide-element",
    "-fontsettings",
    "edit-link-plus",
    "page-toc-button",
    "tbfed-pagefooter",
    "back-to-top-button",
    "expandable-chapters-small",
    "book-summary-scroll-position-saver"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "github": {
      "url": "https://github.com/april-projects/.docs"
    },
    "insert-logo": {
      "url": "https://tencent.cos.mobaijun.com/img/icon/logo.png",
      "style": "background: none; max-height: 100px; min-height: 30px"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link",
        "div.vpower.txt-right"
      ]
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © 版权信息",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "atoc": {
      "addClass": true,
      "className": "atoc"
    },
    "page-treeview": {
      "copyright": "Copyright © aleen42",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "favicon": {
      "shortcut": "https://cdn.jsdelivr.net/gh/april-projects/.docs/img/favicon.ico",
      "bookmark": "https://cdn.jsdelivr.net/gh/april-projects/.docs/img/favicon.ico",
      "appleTouch": "https://cdn.jsdelivr.net/gh/april-projects/.docs/img/apple-touch-icon.png",
      "appleTouchMore": {
        "120x120": "https://cdn.jsdelivr.net/gh/april-projects/.docs/img/android-chrome-192x192.png",
        "180x180": "https://cdn.jsdelivr.net/gh/april-projects/.docs/img/android-chrome-192x192.png"
      }
    },
    "gtalk": {
      "clientID": "f0da19fb826f620b26d6",
      "clientSecret": "666fa5dabfb7913cf38b23fd348cf75d6f673820",
      "repo": ".docs",
      "owner": "april-projects",
      "admin": [
        "mobaijun"
      ]
    },
    "donate": {
      "wechat": "https://tencent.cos.mobaijun.com/img/icon/wechat.png",
      "alipay": "https://tencent.cos.mobaijun.com/img/icon/alipay.png",
      "title": "",
      "button": "赞赏",
      "alipayText": "支付宝捐赠",
      "wechatText": "微信捐赠"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.min.css"
      ],
      "lang": {
        "dockerfile": "docker",
        "conf": "yaml",
        "c++": "c"
      },
      "ignore": [
        "mermaid",
        "eval-js"
      ]
    },
    "minifier": {
      "html": {},
      "css": {},
      "js": {}
    },
    "edit-link-plus": {
      "base": "https://github.com/april-projects/.docs/issues",
      "label": "点我反馈"
    }
  }
}